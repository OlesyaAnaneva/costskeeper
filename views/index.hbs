<button class="btn waves-effect #ef9a9a red lighten-3 btn-large" type="submit" name="action">Talk
  <i class="material-icons right">mic</i>
</button>

<h3 id="content"></h3>
<p>Say just two words... like "cola" and "fifty"</p>
<table class="highlight">
  <thead>
    <tr>
      <th>Item Name</th>
      <th>Item Price</th>
    </tr>
  </thead>

  <tbody class="items">
    {{#each words}}
    <tr>
      <td>{{this.name}}</td>
      <td>{{this.price}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

<br>
